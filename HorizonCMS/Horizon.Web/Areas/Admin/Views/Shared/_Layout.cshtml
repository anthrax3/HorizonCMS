            </nav>
        </div>
        <div class="row height_100">
            <div class="col-xs-12 col-sm-12 col-md-2 col-lg-2 panel-left height_100 f_left">
                @Html.Partial("Partials/_UserProfile")
                @Html.Partial("Partials/_AdminSections")
            </div>
            <div class="col-xs-12 col-sm-12 col-md-10 col-lg-10" style="background: #f5f5f5;">
                @RenderBody()
            </div>
        </div>
    </div>
 
  
