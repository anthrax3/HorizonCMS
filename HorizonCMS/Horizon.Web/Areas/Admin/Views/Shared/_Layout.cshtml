    <meta name="viewport" content="width=device-width, initial-scale=1.0"><!-- SEO TAGS -->
    <meta name="description" content="@ViewBag.Description" />
    <meta name="generator" content="@ViewBag.Generator" />
    <meta name="author" content="Horizon CMS" />
    <meta name="keywords" content="@ViewBag.Keywords" />
    <meta name="canonical" content="www.smtraductores.com" />
