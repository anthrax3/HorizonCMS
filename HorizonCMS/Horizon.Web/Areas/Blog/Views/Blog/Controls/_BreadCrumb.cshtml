@model Horizon.Web.Models.BreadCrumb
@if (ViewBag.BreadCrumb != null)
{
    <ol class="breadcrumb">

        @foreach (var s in Model.parameters)
        {
            if (Model.parameters.IndexOf(s) == 2)
            {
                <li class="active">@s</li>
            }
            else
            {
                <li><a href="#">@s</a></li>
            }
        }
    </ol>
}
